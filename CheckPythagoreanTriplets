package java_class.Assigments;

public class PythagoreanTriplets {
	
	public static void main(String[] args) {
		
		int a = 2;
		int b = 2;
		int c = 2;
		
		if (a*a+b*b==c*c) {
			
			System.out.println("It's Pythagorean Triplet");
		}
		else
			System.out.println("It's  Not a Pythagorean Triplet");
	}

}
